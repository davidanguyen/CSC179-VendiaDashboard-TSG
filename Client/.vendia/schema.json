{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Employee",
  "description": "Detailed List of Employees",
  "type": "object",
  "properties": {
    "Employees": {
      "description": "Employee Details",
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "EmployeeID": {
            "description": "Unique identifier for an employee",
            "type": "integer"
          },
          "First": {
            "description": "First name of employee",
            "type": "string"
          },
          "Last": {
            "description": "Last name of employee",
            "type": "string"
          },
          "Age": {
            "description": "Age of employee",
            "type": "integer"
          },
          "Gender": {
            "description": "Gender of employee",
            "type": "string"
          },
          "Height": {
            "description": "Height of employee",
            "type": "integer"
          },
          "Weight": {
            "description": "Weight of employee",
            "type": "integer"
          },
          "BodyTemp": {
            "description": "Body Temperature of employee",
            "type": "integer"
          },
          "PulseRate": {
            "description": "Pulse Rate of employee",
            "type": "integer"
          },
          "BloodPressure": {
            "description": "Blood Pressure of employee",
            "type": "integer"
          },
          "RespirationRate": {
            "description": "Respiration Rate of employee",
            "type": "integer"
          },
          "ExcerciseAvgPerWeek": {
            "description": "Average Excercise of employee",
            "type": "integer"
          },
          "WorkAvgPerWeek": {
            "description": "Work Average of employee",
            "type": "integer"
          },
          "VacationBalance": {
            "description": "Vacation Balance of employee",
            "type": "integer"
          }
        },
        "required": [
          "EmployeeID",
          "First",
          "Last",
          "Age",
          "Gender",
          "Height",
          "Weight",
          "BodyTemp",
          "PulseRate",
          "BloodPressure",
          "RespirationRate",
          "ExcerciseAvgPerWeek",
          "WorkAvgPerWeek",
          "VacationBalance"
        ]
      }
    }
  }
}